 bool isSameTree(TreeNode* p, TreeNode* q) {
       if(p==NULL && q==NULL){
           return true;
       } 
       if(p==NULL || q==NULL){
           return false;
       }
       if(p->val!=q->val){
           return false;
       }
       bool check1= isSameTree(p->left,q->left);
       bool check2=  isSameTree(p->right,q->right);
       return check1 && check2;
    }
